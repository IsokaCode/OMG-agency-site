{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "onmygrind.com" }
      ],
      "destination": "https://onmygrind.agency/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "omgagency.com" }
      ],
      "destination": "https://onmygrind.agency/$1",
      "permanent": true
    }
  ]
} 